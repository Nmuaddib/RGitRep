f.name_lst <- function(lt, df) {
prest <- names(lt)
anomes_vec <- df %>%
filter(CODIGO == prest) %>%
select(ANOMES)
idx_f <- length(anomes_vec[[1]])
idx_i <- idx_f - (length(lt[[1]]) - 1)
anomes_vec <- anomes_vec[[1]][idx_i:idx_f]
return(anomes_vec)
}
